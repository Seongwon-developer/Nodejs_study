React의 반복문 문법

class App extends Component{
  render(){
    return(
      <div>
        {customers.map(c=>{                 // 이 부분이 반복문이다.
          return <Customer key={c.id} id={c.id} image={c.image} birthday={c.birthday} gender={c.gender} job={c.job} />
        })}
      </div>    
    );
  };
}

